mixin slider(name, bullet, slider, value)
    .container
        .range-slider
            p= name
            span(id=`${bullet}` class=`rs-label` value="0" min="0" max="100") #{value}
            input(id=`${slider}` class="rs-range" name=`${slider}` type='range' value=`${value}` min='0' max='100')
        .box-minmax
            span 0
            span 100
